{
  // tab几个缩进
  "editor.tabSize": 2,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
],
"fileheader.customMade": {
    "description": "",
    "author": "liguanlin<liguanlin@4paradigm.com>",
  },
  "fileheader.cursorMode": {
    "custom_string_obkoro2": "",
    "param": "",
    "return": ""
  },
  "fileheader.configObj": {
    "autoAdd": true,
    "autoAlready": true,
    "dateFormat": "YYYY-MM-DD",
    "wideNum": 0,
    "functionWideNum": 0,
    "colon": [
      " ",
      ": "
    ], // 更改所有文件的注释冒号
    "specialOptions": {
      "Date": "since",
      "LastEditTime": "updated_at",
    },
    "language": {
      // 普通后缀文件
      "js": {
        "head": "/**",
        "middle": " * @",
        "end": " */",
        // 函数自定义注释符号：如果有此配置 会默认使用
        "functionSymbol": {
          "head": "/** ", // 统一增加几个*号
          "middle": " * @",
          "end": " */"
        }
      },
    }
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
"bookmarks.useWorkaroundForFormatters": true,
"editor.formatOnSave": true,
}
